import random
caratteri="+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"

lunghezza= int(input("quanti numeri deve eseere lunga?"))

password_autogenerata= ""

for i in range(lunghezza):
    carattere_casuale = random.choice(caratteri)
    password_autogenerata += carattere_casuale

print(password_autogenerata)
